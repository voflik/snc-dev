        <sys_ui_annotation action="INSERT_OR_UPDATE">
            <is_plain_text>false</is_plain_text>
            <sys_created_by>ben.hollifield@snc_SHARE</sys_created_by>
            <sys_created_on>2015-11-18 03:22:49</sys_created_on>
